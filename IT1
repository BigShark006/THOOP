import java.util.*;

public class Main {

    static long fib(int n) {
        if (n <= 2) return 1;
        long a = 1, b = 1;
        for (int i = 3; i <= n; i++) {
            long c = a + b;
            a = b;
            b = c;
        }
        return b;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        while (sc.hasNextInt()) {
            int n = sc.nextInt();
            System.out.println(fib(n));
        }
    }
}