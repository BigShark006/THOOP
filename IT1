import java.util.*;

public class Main {

    static long solve(int n) {
        if (n == 1) return 0;
        if (n == 2) return 1;
        return (1L << (n - 2)) - 1;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();

        System.out.println(solve(a));
        System.out.println(solve(b));
    }
}